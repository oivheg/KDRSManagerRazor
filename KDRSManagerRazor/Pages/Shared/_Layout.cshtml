        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.8.0/js/bootstrap-datepicker.js" type="text/javascript"></script>
        <script>
            $('.input-daterange').datepicker({
                maxViewMode: 2,
                todayBtn: "linked",
                language: "no",
                calendarWeeks: true,
                autoclose: true,
                todayHighlight: true,
                format: "dd/mm/yyyy",
                toggleActive: true
            });
        </script>
        <script src="~/js/Global.js"></script>
        <script type="text/javascript">
    $(document).ready(function () {
        console.log('this is run on page load');

        @*$('#dateFrom').val( " @Model.From.ToString()");*@
        @* $('#dateTo').val("@Model.To");*@
        $('#dateFrom').val(getDate('from'));
        $('#dateTo').val(getDate('to'));

    });
        </script>
