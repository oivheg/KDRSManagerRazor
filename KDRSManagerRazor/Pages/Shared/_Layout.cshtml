        <nav class="navbar  navbar-light fixed-top">
            <a class="navbar-brand " href="#">KDRS Monitor</a>
            <div class="dropdown">
                <button class="btn btn-danger btn-secondary dropdown-toggle" type="button" data-toggle="dropdown" aria-expanded="false">Settings</button>
                <div class="dropdown-menu">
                    <a class="dropdown-item" href="/Index">Home</a>
                    <a class="dropdown-item" href="/Server">Server</a>
                </div>
            </div>
            @*<button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarsExample01" aria-controls="navbarsExample01" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
